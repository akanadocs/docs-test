<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<metadataconfig xmlns="http://soa.com/xsd/metadataconfig/1.0">
	<!-- When API use Licenses then by default all files are restricted.
		Hence all unrestricted entries should be added to metadata.xml and assign a public license.
	-->
	<metadata>
		<target>documents/index.html</target>
		<li>censes>
			<li>cense name="public-license-1"/>
		</licenses>
	</metadata>

	<metadata>
		<target>documents/tagging-by-file.html</target>
		<li>censes>
			<li>cense name="public-license-1"/>
		</licenses>
	</metadata>

	<metadata>
		<target>documents/tagging-within-document.html</target>
		<li>censes>
			<li>cense name="public-license-1"/>
		</licenses>
	</metadata>

	<metadata>
		<target>documents/replacements.js</target>
		<li>censes>
			<li>cense name="public-license-1"/>
		</licenses>
	</metadata>
	
	<metadata>
		<target>documents/tagging-within-document-with-replacements.html</target>
		<li>censes>
			<li>cense name="public-license-1"/>
		</licenses>
	</metadata>

	<metadata>
		<target>documents/site-admin.html</target>
		<roles>
			<role name="SiteAdmin"/>
		</roles>
	</metadata>

	<metadata>
		<target>documents/business-admin.html</target>
		<roles>
			<role name="BusinessAdmin"/>
		</roles>
	</metadata>

	<metadata>
		<target>documents/api-admin.html</target>
		<roles>
			<role name="Admin"/>
		</roles>
	</metadata>

	<metadata>
		<target>documents/site-admin-or-business-admin.html</target>
		<roles>
			<role name="BusinessAdmin"/>
			<role name="SiteAdmin"/>
		</roles>
	</metadata>

	<!-- not supported and behaves as SiteAdmin or BusinessAdmin.-->
	<metadata>
		<target>documents/site-admin-and-business-admin.html</target>
		<roles>
			<role name="BusinessAdmin"/>
			<role name="SiteAdmin"/>
		</roles>
	</metadata>

	<metadata>
		<target>documents/public-scope-1.html</target>
		<scopes>
			<scope name="public-scope-1"/>
		</scopes>
	</metadata>

	<metadata>
		<target>documents/two-public-scopes.html</target>
		<scopes>
			<scope name="public-scope-1"/>
			<scope name="public-scope-2"/>
		</scopes>
	</metadata>

	<metadata>
		<target>documents/private-scope-1.html</target>
		<scopes>
			<scope name="private-scope-1"/>
		</scopes>
	</metadata>

	<metadata>
		<target>documents/public-and-private-scope.html</target>
		<scopes>
			<scope name="public-scope-1"/>
			<scope name="private-scope-1"/>
		</scopes>
	</metadata>

	<metadata>
		<target>documents/two-private-scopes.html</target>
		<scopes>
			<scope name="private-scope-1"/>
			<scope name="private-scope-2"/>
		</scopes>
	</metadata>
	
	<metadata>
		<target>documents/public-license-1.html</target>
		<li>censes>
			<li>cense name="public-license-1"/>
		</licenses>
	</metadata>
	<metadata>
		<target>documents/public-license-2.html</target>
		<li>censes>
			<li>cense name="public-license-2"/>
		</licenses>
	</metadata>
	<metadata>
		<target>documents/private-license-1.html</target>
		<li>censes>
			<li>cense name="private-license-1"/>
		</licenses>
	</metadata>

	<metadata>
		<target>documents/operation-targeted.html</target>
		<operations>
			<operation name="operation-targeted"/>
		</operations>
	</metadata>
</metadataconfig>
