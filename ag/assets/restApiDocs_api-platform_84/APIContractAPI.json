{"swagger":"2.0","info":{"title":"APIContractAPI","description":"<p>The Contracts service provides operations for managing API access contracts in the Akana API Platform.</p>","version":"0.0.0"},"host":"0.0.0.0","basePath":"/api","schemes":["http","https"],"tags":[{"name":"APIContractAPI"}],"paths":{"/contracts/{ContractID}/appdetails":{"get":{"tags":["APIContractAPI"],"operationId":"getContractAppDetails","produces":["application/xml","application/json","application/vnd.soa.v71+json","application/vnd.soa.v72+json","application/vnd.soa.v80+json","application/vnd.soa.v71+xml","application/vnd.soa.v72+xml","application/vnd.soa.v80+xml","application/vnd.soa.v81+xml","application/vnd.soa.v81+json"],"parameters":[{"in":"path","name":"ContractID","type":"string","required":true}],"responses":{"default":{"schema":{"$ref":"#/definitions/ContractApplication"}}},"deprecated":false}},"/contracts/{ContractDN}/scope":{"put":{"tags":["APIContractAPI"],"operationId":"saveContractScope","consumes":["application/json","application/xml","application/vnd.soa.v71+json","application/vnd.soa.v72+json","application/vnd.soa.v80+json","application/vnd.soa.v71+xml","application/vnd.soa.v72+xml","application/vnd.soa.v80+xml","application/vnd.soa.v81+xml","application/vnd.soa.v81+json"],"produces":["application/json","application/xml","application/vnd.soa.v71+json","application/vnd.soa.v72+json","application/vnd.soa.v80+json","application/vnd.soa.v71+xml","application/vnd.soa.v72+xml","application/vnd.soa.v80+xml","application/vnd.soa.v81+xml","application/vnd.soa.v81+json"],"parameters":[{"in":"path","name":"ContractDN","type":"string","required":true},{"in":"body","name":"parameter0","schema":{"$ref":"#/definitions/ContractScope"}}],"responses":{"default":{"schema":{"$ref":"#/definitions/Contract"}}},"deprecated":false}},"/contracts/{ContractID}/apidetails":{"get":{"tags":["APIContractAPI"],"operationId":"getContractAPIDetails","produces":["application/xml","application/json","application/vn